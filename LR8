def IsChislo(el):
    #функция проверят является ли символ числом
    try:
        x = int(el)
        return True
    except:
        return False

str = input()
flag = True
#перебираем все предложения
for s in str.split('.'):
    flag = True
    #перебор всех символов в каждом предложении
    for i in range(0, len(s) - 1):
        #если 2 подряд идущих числа являются числами
        if IsChislo(s[i]) and IsChislo(s[i + 1]):
            if i + 2 < len(s):
                if not IsChislo(s[i + 2]):
                    flag = False
                    break
            else:
                flag = False
                break
    if flag:
        print(s)
